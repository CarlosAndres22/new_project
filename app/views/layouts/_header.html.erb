<header id="main_header" class="cf">
	<div class="wrapper_wide">
		<%= link_to "Funevenful", root_path, id: "logo" %>
		<nav>
			<ul>
				<% unless evento_signed_in? %>
					<li><%= link_to "Explore", eventos_path %></li>
					<li><%= link_to "Sign In", new_evento_session_path %></li>
					<li><%= link_to "Create an event", new_evento_registration_path %></li>
				<% else %>
					<li><%= link_to "Dashboard", root_path %></li>
					<li><%= link_to "Explore", eventos_path %></li>
					<li><%= link_to "Sign Out", destroy_evento_session_path, method: :delete %></li>
				<% end %>
			</ul>
		</nav>
	</div>
</header>